+                if (walkTarget.getTarget().currentBlockPosition().distSqr(this.lastTargetPos) > 4.0D) {
+                    this.start(level, owner, gameTime);
+                }
+            }
+        } else {
+        // DeerFolia end
         Path path = owner.getNavigation().getPath();
         Brain<?> brain = owner.getBrain();
         if (this.path != path) {
